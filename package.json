{
  "name": "spotify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:load": "NEXT_CPU_PROF=1 next dev",
    "preinstall": "npx npm-force-resolutions",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "watch": "graphql-codegen --watch",
    "tailwind:build": "tailwindcss -i ./src/styles/globals.css -o ./src/styles/globals.output.css --minify",
    "tailwind:watch": "tailwindcss -i ./src/styles/globals.css -o ./src/styles/globals.output.css --watch ",
    "codegen": "graphql-codegen --config codegen.ts --verbose",
    "codegen:watch": "graphql-codegen --watch",
    "prepare": "husky install"
  },
  "dependencies": {
