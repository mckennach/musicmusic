    "postcss": "^8",
    "prettier": "3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "use-resize-observer": "^9.1.0"
  },
  "lint-staged": {
    "src/*.{js,jsx,ts,tsx,md,html}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx,md,html}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
